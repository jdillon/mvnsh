<?xml version="1.0" encoding="UTF-8"?>
<configuration>
  <appender name="console" class="ch.qos.logback.core.ConsoleAppender">
    <encoder>
      <pattern>[%level] %m%n</pattern>
    </encoder>
  </appender>

  <root level="${shell.logging}">
    <appender-ref ref="console"/>
  </root>
</configuration>
